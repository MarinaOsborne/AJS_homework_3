{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "no-undef",
      "LabeledStatement",
      "WithStatement"
    ],
    "linebreak-style": 0,
    "global-require": 0,
    "no-undef": 0,
    "class-methods-use-this": 0
  }
}